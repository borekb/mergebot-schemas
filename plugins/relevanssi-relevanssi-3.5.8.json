{
    "name": "Relevanssi - A Better Search",
    "version": "3.5.8",
    "url": "https:\/\/wordpress.org\/plugins\/relevanssi",
    "primaryKeys": {
        "relevanssi": {
            "key": [
                "doc",
                "term",
                "item"
            ],
            "auto_increment": false
        },
        "relevanssi_log": {
            "key": [
                "id"
            ]
        },
        "relevanssi_stopwords": {
            "key": [
                "stopword"
            ],
            "auto_increment": false
        }
    },
    "foreignKeys": {
        "relevanssi_log:user_id": "users:ID"
    },
    "ignore": [
        {
            "relevanssi": "(.*)"
        },
        {
            "relevanssi_log": "(.*)"
        },
        {
            "relevanssi_stopwords": "(.*)"
        }
    ]
}